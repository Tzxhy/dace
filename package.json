{
  "name": "unjs",
  "description": "A minimalistic universal (isomorphic) react framework for building web apps",
  "version": "3.2.4",
  "repository": "un-js/unjs",
  "keywords": [
    "universal", "isomorphic", "webpack", "react",
    "redux", "server-side-rendering", "koa", "code-splitting"
  ],
  "scripts": {
    "start": "BABEL_ENV=server NODE_ENV=local babel-node scripts/start.js",
    "start:dist": "NODE_ENV=production node prd/server.js",
    "build:client": "babel-node scripts/build.js --host=client",
    "build:server": "babel-node scripts/build.js --host=server",
    "build:all": "npm run build:client && npm run build:server",
    "stylelint": "stylelint --fix \"src/**/*.(css|less|sass|scss)\"",
    "eslint": "eslint --fix src",
    "lint": "npm run stylelint && npm run eslint",
    "postrelease": "git push && git push origin --tags",
    "release": "standard-version && npm publish"
  },
  "dependencies": {
    "asset-require-hook": "^1.2.0",
    "axios": "^0.18.0",
    "babel-cli": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-syntax-import-async-component-sync": "^1.0.0",
    "babel-plugin-syntax-object-rest-spread": "^6.13.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "babel-preset-react": "^6.24.1",
    "clean-webpack-plugin": "^0.1.19",
    "css-loader": "^0.28.11",
    "css-modules-require-hook": "^4.2.3",
    "eslint": "^4.19.1",
    "eslint-config-qunar": "^3.0.0",
    "eslint-loader": "^2.0.0",
    "file-loader": "^1.1.11",
    "koa": "^2.5.1",
    "koa-proxies": "^0.7.0",
    "koa-static": "^4.0.3",
    "koa-webpack": "^4.0.0",
    "mini-css-extract-plugin": "^0.4.0",
    "postcss-loader": "^2.1.5",
    "prop-types": "^15.6.1",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "react-helmet": "^5.2.0",
    "react-redux": "^5.0.7",
    "react-router": "^4.2.0",
    "react-router-config": "^1.0.0-beta.4",
    "react-router-dom": "^4.2.2",
    "redux": "^4.0.0",
    "redux-thunk": "^2.3.0",
    "serialize-javascript": "^1.5.0",
    "stylelint": "^9.2.1",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-webpack-plugin": "^0.10.5",
    "webpack": "^4.9.1",
    "webpack-cli": "^2.1.4",
    "webpack-merge": "^4.1.2",
    "webpack-node-externals": "^1.7.2"
  },
  "devDependencies": {
    "redux-devtools": "^3.4.1"
  }
}
