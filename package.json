{
  "name": "unjs",
  "description": "A minimalistic universal (isomorphic) react framework for building web apps",
  "version": "1.0.0",
  "repository": "un-js/unjs",
  "keywords": [
    "universal",
    "isomorphic",
    "webpack",
    "react",
    "redux",
    "server-side-rendering",
    "koa",
    "code-splitting"
  ],
  "bin": {
    "unjs": "src/bin/unjs.js"
  },
  "scripts": {
    "build": "rm -rf lib && babel src -d lib",
    "stylelint": "stylelint --fix \"src/**/*.(css|less|sass|scss)\"",
    "eslint": "eslint --fix src",
    "test": "node test/index.js",
    "lint": "npm run stylelint && npm run eslint",
    "prepare": "npm run build",
    "postrelease": "git push && git push origin --tags",
    "release": "standard-version && npm publish"
  },
  "dependencies": {
    "asset-require-hook": "^1.2.0",
    "axios": "^0.18.0",
    "babel-cli": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-syntax-import-async-component-sync": "^1.0.0",
    "babel-plugin-syntax-object-rest-spread": "^6.13.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "babel-preset-react": "^6.24.1",
    "clean-webpack-plugin": "^0.1.19",
    "commander": "^2.15.1",
    "css-loader": "^0.28.11",
    "css-modules-require-hook": "^4.2.3",
    "eslint": "^4.19.1",
    "eslint-config-qunar": "^3.0.0",
    "eslint-loader": "^2.0.0",
    "file-loader": "^1.1.11",
    "koa": "^2.5.1",
    "koa-proxies": "^0.7.0",
    "koa-static": "^4.0.3",
    "koa-webpack": "^4.0.0",
    "loader-utils": "^1.1.0",
    "mini-css-extract-plugin": "^0.4.0",
    "postcss-loader": "^2.1.5",
    "prop-types": "^15.6.1",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "react-helmet": "^5.2.0",
    "react-redux": "^5.0.7",
    "react-router": "^4.2.0",
    "react-router-config": "^1.0.0-beta.4",
    "react-router-dom": "^4.2.2",
    "redux": "^4.0.0",
    "redux-thunk": "^2.3.0",
    "serialize-javascript": "^1.5.0",
    "stylelint": "^9.2.1",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-webpack-plugin": "^0.10.5",
    "webpack": "^4.9.1",
    "webpack-cli": "^2.1.4",
    "webpack-merge": "^4.1.2",
    "webpack-node-externals": "^1.7.2"
  },
  "devDependencies": {
    "mocha": "^5.2.0",
    "redux-devtools": "^3.4.1",
    "should": "^13.2.1",
    "supertest": "^3.1.0"
  }
}
